  "private": true,
  "scripts": {
    "start": "run-s dev",
    "convert-center": "node scripts/map-center-converter.js src/libs/maps/concession-centers.json",
    "dev": "cross-env NODE_ENV=development BABEL_ENV=development BUILD_PROFILE_NAME=dev webpack-dev-server --config webpack.dev.js",
    "debug": "cross-env NODE_ENV=development BABEL_ENV=debug BUILD_PROFILE_NAME=dev node --max-old-space-size=4096 node_modules/.bin/webpack-dev-server --config webpack.debug.js",
    "build": "run-s clean build:webpack",
    "analyze": "run-s clean build:analyze",
    "build:webpack": "cross-env NODE_ENV=production BABEL_ENV=production BUILD_PROFILE_NAME=dev webpack --config webpack.prod.js",
    "build:analyze": "cross-env NODE_ENV=production BABEL_ENV=production BUILD_PROFILE_NAME=dev webpack --config webpack.analyze.js",
    "clean": "rimraf dist .babel-helpers-source",
    "lint": "prettier --write \"src/**/*.@(js|jsx|json|scss|css|gql|graphql)\" && eslint_d --fix src",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/!(fonts|icons)/**/*.@(js|jsx|json)": [
      "prettier --write",
      "eslint_d --fix",
      "git add"
    ],
    "src/!(fonts|icons)/**/*.@(scss|css|gql|graphql)": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 versions",
